@article{Parker-Wood2014,
  abstract = {File names are one of the earliest computing$\backslash$nabstractions, a string of characters to uniquely$\backslash$nidentify a file for the system, and to help users$\backslash$nremember the contents when they look for it later. They$\backslash$nare also a rich source of semantic metadata about$\backslash$nfiles. However, this metadata is unstructured and$\backslash$nopaque to the rest of the system. As a result, metadata$\backslash$nin file names is often error-prone, and hard to search$\backslash$nfor. File names can and should be more meaningful and$\backslash$nreliable, while simplifying application design and$\backslash$nencouraging users and applications to provide more$\backslash$nmetadata for search. We describe a POSIX compliant$\backslash$nprototype file system, TrueNames, which demonstrates an$\backslash$nalternate approach to naming and metadata, called$\backslash$nmetadata aware naming. TrueNames separates the task of$\backslash$nuniquely identifying a file from the task of helping$\backslash$nthe user remember its contents. It captures metadata in$\backslash$na structured format for later indexing, and uses it to$\backslash$ngenerate file names which are correct, regenerable, and$\backslash$ndisambiguatable by design. TrueNames simplifies$\backslash$napplication design by providing a consistent interface$\backslash$nfor metadata aware naming, incurs a low overhead of$\backslash$napproximately 15{\%} under realistic workloads, and can$\backslash$nsimplify a wide variety of data management tasks for$\backslash$nboth applications and users.},
  author   = {Parker-Wood, Aleatha and Long, Darrell D E and Miller, Ethan and Rigaux, Philippe and Isaacson, Andy},
  file     = {:C$\backslash$:/Users/Tony/Dropbox/personal/research statement/semantic fs/Managing File Names with Metadata.pdf:pdf},
  isbn     = {9781450329200},
  journal  = {Proceedings of International Conference on Systems and Storage},
  pages    = {1--11},
  title    = {{A File By Any Other Name: Managing File Names with Metadata}},
  year     = {2014}
}

@article{Prabhakaran2005,
  abstract = { We propose a novel method to measure the robustness of journaling file systems under disk write failures. In our approach, we build models of how journaling file systems order disk writes under different journaling modes and use these models to inject write failures during file system updates. Using our technique, we analyze if journaling file systems maintain on-disk consistency in the presence of disk write failures. We apply our technique to three important Linux journaling file systems: ext3, Reiserfs, and IBM JFS. From our analysis, we identify several design flaws and correctness bugs in these file systems, which can cause serious file system errors ranging from data corruption to unmountable file systems.},
  author   = {Prabhakaran, Vijayan and Arpaci-Dusseau, Andrea C. and Arpaci-Dusseau, Remzi H.},
  doi      = {10.1109/DSN.2005.65},
  file     = {:C$\backslash$:/Users/Tony/Dropbox/personal/research statement/semantic fs/Analysis and Evolution of Journaling File Systems.pdf:pdf},
  isbn     = {0-7695-2282-3},
  journal  = {Proceedings of the International Conference on Dependable Systems and Networks},
  pages    = {8},
  title    = {{Analysis and evolution of journaling file systems}},
  url      = {http://dl.acm.org/citation.cfm?id=1247360.1247368},
  year     = {2005}
}

@article{Schandl2009,
  abstract = {One of the main characteristics of Linked Open Data (LOD) is the exclusive application of standards published and main- tained by the World Wide Web Consortium. This strict adherence is kept on all levels, ranging from the identification and transportation (URI, HTTP) to the interpretation (RDF, RDFS, OWL) of resource descriptions. Because these standards are open and accessible to everybody, broad acceptance and proliferation of LOD technologies in Web-based applications and services are enabled. On typical desktops, however, the majority of applications are not aware of Web standards, but use hierarchical file systems to organize and store information. This results in a gap between the two distinct information spaces of the Web and the desktop. To bridge this gap, we propose a virtual file system representation of LOD sets, through which they can be accessed as if they were present in the file system and thus easily be used within desktop applications.},
  author   = {Schandl, Bernhard},
  file     = {:C$\backslash$:/Users/Tony/Dropbox/personal/research statement/semantic fs/Representing Linked Data as Virtual File Systems.pdf:pdf},
  issn     = {16130073},
  journal  = {CEUR Workshop Proceedings},
  keywords = {File systems,Information representation,Linked Open Data,Semantic desktop},
