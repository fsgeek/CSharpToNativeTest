@article{Min2015,
  abstract = {Today, systems software is too complex to be bug-free. To find bugs in systems software, developers often rely on code checkers, like Sparse in Linux. However, the capability of existing tools used in commodity, large-scale systems is limited to finding only shallow bugs that tend to be introduced by the simple mistakes of programmers and require no deep understanding of code. Unfortunately, a majority of and difficult-to-find bugs are semantic ones, which violate high-level rules or invariants (e.g., missing a permission check). Thus, it is difficult for code-checking tools that lack the understanding of a programmer's true intention, to reason about semantic correctness. To solve this problem, we present JUXTA, a tool that au-tomatically infers high-level semantics directly from source code. The key idea of JUXTA is to compare and contrast mul-tiple existing implementations that obey latent yet implicit high-level semantics. For example, the implementation of open() at the file system layer expects to handle an out-of-space error of the disk, regardless of implementation. We have applied JUXTA to 54 file systems in the stock Linux kernel (680K LoC), found 139 previously unknown semantic bugs (one bug per 4.9K LoC), and provided corresponding patches to 41 different file systems that include mature, popu-lar file systems like ext4, btrfs, xfs and nfs. These semantic bugs are not easy to locate, as all that JUXTA found have ex-isted for over 6.4 years on average. Not only do our empirical results look promising, but the design of JUXTA is generic (not specific to file systems) enough to be easily extended to any software that has multiple implementations, like browsers or network stacks. [Copyright notice will appear here once 'preprint' option is removed.]},
  author   = {Min, Changwoo and Kashyap, Sanidhya and Lee, Byoungyoung and Song, Chengyu and Kim, Taesoo},
  doi      = {10.1145/2815400.2815422},
  file     = {:C$\backslash$:/Users/Tony/Dropbox/personal/research statement/semantic fs/Cross-checking Semantic Correctness The Case of Finding File System Bugs.pdf:pdf},
  isbn     = {9781450338349},
  journal  = {Sosp '15},
  pages    = {361--377},
  title    = {{Cross-checking Semantic Correctness : The Case of Finding File System Bugs}},
  year     = {2015}
}
